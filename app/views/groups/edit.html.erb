<h2> Edit <%= link_to @group.title, @group%></h2>
<h3> Change group name </h3>
<%= form_for @group do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <%= form.submit %>
<% end %>
<%=form_for @group, method: :delete do |form| %>
  <%= form.submit "Delete" %>
<% end %>
